
pipeline{
  agent{
         dockerfile true
         }
  stages{
        stage('Example')
        {
          steps{
                 echo 'Hello World'
               }
        }
        stage('Build Docker')
        {
          steps{
                 sh '''
                       docker run ubuntu
                       docker build -t myimage1:1.01 .
                       docker run myimage1:1.01
                 '''
               }
        }
  }
}
